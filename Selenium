// Include imports:
import org.openqa.selenium.By; // Used to locate elements in a document 
import org.openqa.selenium.WebDriver; // Used for the WebDriver (Drives a browser natively like a user)
import org.openqa.selenium.WebElement; 
import org.openqa.selenium.chrome.ChromeDriver; // Used for the ChromeDriver (Seperate executable used to control Chrome) 

public class Main{
  public static void main(String[] args){
    System.setProperty("webdriver.chrome.driver", "C: ..."); // Insert the file path for the chrome driver, e.g. \Users\amat001\Desktop\Java\chromedriver.exe

    // Set up the webdriver object
    WebDriver driver = new ChromeDriver();

    // Set the URL:
    String url = "...."; // Insert the URL you are testing on the mlb.com website

    // Let the driver object get the page
    driver.get(url);

    // Print the HTMl of the page:
    System.out.println(driver.getPageSource()); // Selenium will return the HTML as a string; use JSoup to parse through the string

    // Close the driver:
    driver.quit(); 
  }
}
